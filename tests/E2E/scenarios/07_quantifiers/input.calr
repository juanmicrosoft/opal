§F{f003:ImplicationTest:pub}
  §I{i32:x}
  §O{bool}
  §Q (-> (> x INT:0) (>= x INT:0))
  §R true
§/F{f003}

§F{f004:MatrixSymmetry:pub}
  §I{i32:n}
  §I{i32:m}
  §O{bool}
  §Q (forall ((i i32) (j i32)) (-> (&& (>= i INT:0) (< i n) (>= j INT:0) (< j m)) (>= i INT:0)))
  §R true
§/F{f004}


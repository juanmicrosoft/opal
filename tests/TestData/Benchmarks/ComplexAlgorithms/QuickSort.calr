§M{m001:QuickSort}
§F{f001:ChoosePivot:pub}
  §I{i32:low}
  §I{i32:high}
  §O{i32}
  §Q (>= low 0)
  §Q (<= low high)
  §R high
§/F{f001}
§F{f002:ShouldSwap:pub}
  §I{i32:value}
  §I{i32:pivot}
  §O{bool}
  §R (<= value pivot)
§/F{f002}
§F{f003:NextPartitionIndex:pub}
  §I{i32:current}
  §O{i32}
  §R (+ current 1)
§/F{f003}
§F{f004:IsSortedPair:pub}
  §I{i32:a}
  §I{i32:b}
  §O{bool}
  §R (<= a b)
§/F{f004}
§/M{m001}

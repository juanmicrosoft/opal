§/F{f002}

§F{f003:GetLength:pub}
  §I{str:s}
  §O{i32}
  §R (len s)
§/F{f003}

§F{f004:ToUpperCase:pub}
  §I{str:s}
  §O{str}
  §R (upper s)
§/F{f004}
§/M{m001}

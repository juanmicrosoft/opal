{
  "lspServers": {
    "typescript": {
      "command": "typescript-language-server",
      "args": ["--stdio"],
      "fileExtensions": {
        ".ts": "typescript",
        ".tsx": "typescriptreact",
        ".js": "javascript",
        ".jsx": "javascriptreact"
      }
    },
    "csharp": {
      "command": "OmniSharp",
      "args": ["-lsp"],
      "fileExtensions": {
        ".cs": "csharp"
      }
    },
  }
}

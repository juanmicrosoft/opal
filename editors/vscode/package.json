{
  "name": "calor",
  "displayName": "Calor Language",
  "description": "Syntax highlighting for the Calor programming language",
  "version": "0.1.9",
  "publisher": "calor-dev",
  "license": "Apache-2.0",
  "icon": "icons/calor-icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/juanmicrosoft/calor"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "calor",
        "aliases": [
          "Calor",
          "calor"
        ],
        "extensions": [
          ".calr"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/calor-file-icon.png",
          "dark": "./icons/calor-file-icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "calor",
        "scopeName": "source.calor",
        "path": "./syntaxes/calor.tmLanguage.json"
      }
    ]
  }
}
